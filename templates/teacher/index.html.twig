{% extends 'base_dashboard.html.twig' %}

{% block title %}Hello TeacherController!{% endblock %}

{% block body %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    <div class="example-wrapper">


        This friendly message is coming from:
        <ul>
            {% for user in teacherList %}
                <li>
                    <a href="{{ path('app_teacher_show',{'id':user.Id}) }}"> {{ user.getFullName() }}</a>
                </li>
            {% endfor %}

        </ul>
    </div>
{% endblock %}
